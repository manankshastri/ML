    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "def softmax(L):\n",
    "    expl = np.exp(L)\n",
    "    suml = sum(expl)\n",
    "    result = []\n",
    "    for i in expl:\n",
    "        result.append(i*1.0 / suml)\n",
    "    return result"
   ]
  },
